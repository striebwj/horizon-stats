{
  "name": "striebwj/horizon-stats",
  "description": "A horizon stats",
  "license": "MIT",
  "keywords": [
    "horizon",
    "stats"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Wade Striebel",
      "email": "wade@striebel.ca"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "laravel/horizon": "^4|^5"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "striebwj\\HorizonStats\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "striebwj\\HorizonStats\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "striebwj\\HorizonStats\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
